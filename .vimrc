execute pathogen#infect()

filetype plugin on

set copyindent

set preserveindent

set expandtab

set shiftwidth=3

set tabstop=3

set t_Co=256

set background=dark

colorscheme hybrid

let g:airline_theme='tomorrow'

set mouse=a

autocmd FileType * setlocal formatoptions-=c formatoptions-=r formatoptions-=o

set ff=unix

set ffs=unix

let python_highlight_all = 1

au FileType python set shiftwidth=4|set tabstop=4

set laststatus=2
